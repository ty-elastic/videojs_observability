{
  "name": "videojs_observability",
  "private": true,
  "version": "0.30.0",
  "description": "OpenTelemetry + Elastic for video player metrics and end-to-end tracing",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --progress --color --port 8090 --config webpack.dev.config.js --hot --host 0.0.0.0 --compress",
    "start-nc": "webpack serve --progress --color --port 8090 --config webpack.dev.config.js --hot --host 0.0.0.0 --no-compress",
    "start-prod": "webpack serve --progress --color --port 8090 --config webpack.prod.config.js --hot --host 0.0.0.0 --compress",
    "start-prodnc": "webpack serve --progress --color --port 8090 --config webpack.prod.config.js --hot --host 0.0.0.0 --no-compress",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ty-elastic/videojs_observability.git"
  },
  "keywords": [
    "opentelemetry",
    "tracing",
    "videojs",
    "elastic",
    "nginx"
  ],
  "engines": {
    "node": ">=14"
  },
  "author": "Ty",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://https://github.com/ty-elastic/videojs_observability/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "babel-loader": "^8.0.6",
    "file-loader": "^6.2.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@opentelemetry/api": "1.1.0",
    "@opentelemetry/api-metrics": "0.30.0",
    "@opentelemetry/context-zone": "1.4.0",
    "@opentelemetry/core": "1.4.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.30.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.30.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.30.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.30.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.30.0",
    "@opentelemetry/exporter-trace-otlp-proto": "0.30.0",
    "@opentelemetry/instrumentation": "0.30.0",
    "@opentelemetry/instrumentation-xml-http-request": "0.30.0",
    "@opentelemetry/propagator-b3": "1.4.0",
    "@opentelemetry/resources": "1.4.0",
    "@opentelemetry/sdk-metrics-base": "0.30.0",
    "@opentelemetry/sdk-trace-base": "1.4.0",
    "@opentelemetry/sdk-trace-web": "1.4.0",
    "@opentelemetry/semantic-conventions": "1.4.0",
    "uuid": "^8.3.2",
    "videojs-event-tracking": "^1.0.3"
  }
}
